pipeline {
    agent any
     stages{

      stage('Clone_project')
      {
 
       steps{
               git branch:'main' , url:'https://github.com/neelakshi1990/Amazon_praveen.git'
            }

      }

      stage('compile')
      {
 
       steps{
               sh 'mvn compile'
            }

      }

      stage('test')
      {
 
       steps{
               sh 'mvn test'
            }

      }

      stage('build')
      {
 
       steps{
               sh 'mvn clean install'
            }

      }

   }
post {
        success {
            emailext(to: 'chatterjee.neelakshi@gmail.com'
            subject: "build successfull",
                body: "The pipeline project successfully ran")
        }
      }
}
